{
  "name": "color-schemer",
  "version": "0.1.0",
  "description": "Manage semantic color schemes and export them",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "bin": {
    "color-schemer": "cli.js"
  },
  "scripts": {
    "test": "./node_modules/jasmine/bin/jasmine.js && jshint *.js",
    "start": "watchify -o editor/js/bundle.js -v -d editor/js/app.js",
    "build": "browserify ./editor -t [envify --NODE_ENV production] | uglifyjs -cm > editor/js/bundle.min.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify",
      "brfs"
    ]
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crucialfelix/color-schemer"
  },
  "author": "Chris Sattinger <crucialfelix@gmail.com> (https://github.com/crucialfelix)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crucialfelix/color-schemer/issues"
  },
  "homepage": "https://github.com/crucialfelix/color-schemer",
  "dependencies": {
    "colorable": "^1.0.5",
    "commander": "^2.8.1",
    "connect": "^3.3.5",
    "flux": "^2.0.3",
    "jquery": "^2.1.4",
    "js-yaml": "^3.3.0",
    "keymirror": "^0.1.1",
    "libxmljs": "^0.14.0",
    "lodash": "^3.7.0",
    "object-assign": "^2.0.0",
    "opn": "^1.0.2",
    "react": "^0.13.2",
    "serve-static": "^1.9.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^10.1.0",
    "envify": "^3.4.0",
    "jasmine": "^2.3.1",
    "jshint": "^2.7.0",
    "reactify": "^1.1.0",
    "uglify-js": "^2.4.21",
    "watchify": "^3.2.1"
  }
}
